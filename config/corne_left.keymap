/ {
    behaviors {
        compatible = "zmk,behaviors";
    };

    keymap {
        compatible = "zmk,keymap";

        default_layer: layer_0 {
            bindings = <
                &kp ESC   &kp Q    &kp W    &kp E    &kp R    &kp T
                &kp TAB   &kp A    &kp S    &kp D    &kp F    &kp G
                &kp LSFT  &kp Z    &kp X    &kp C    &kp V    &kp B
                &kp LCTL  &kp LGUI &kp LALT &kp SPC  &kp NO   &kp NO
            >;
        };
    };
};
